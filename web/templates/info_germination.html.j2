{% extends "root.html.j2" %}
{% block title %}Germination Information{% endblock %}
{% block content %}
<h2>{{ self.title() }}</h2>
<p>Below you will find additional information about the germination codes mentioned in individual species accounts.</p>
{% for item in germination %}
<div id="germ-code-{{ item.id }}" class="p-3 mb-5">
<h3>{{ item.code }}</h3>
    <div>
        <p class="lead">{{ item.summary }}</p>
        <p>{{ item.description }}</p>
    </div>
</div>
{% endfor %}
{% endblock %}

